	<queryString>
		<![CDATA[SELECT ROW_NUMBER() OVER(ORDER BY income.week DESC) AS row,
income.week as startDateWeek, sum(income.sale) as total
FROM
(SELECT date_trunc('quarter', bill.book_date) as week, bill.total as sale
FROM public."user_bill" as bill
WHERE bill.state=1
) as income
GROUP BY income.week
ORDER BY  income.week DESC]]>
	</queryString>
