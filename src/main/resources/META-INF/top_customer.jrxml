	<queryString>
		<![CDATA[SELECT ROW_NUMBER() OVER(ORDER BY bill.total DESC) AS row,
acc.full_name, acc.address, acc.birthday, acc.gender, bill.total
FROM public."user" as us, public."account" as acc,
(SELECT user_bill.id_user, sum(user_bill.total) as total FROM public."user_bill" as user_bill
WHERE user_bill.state=1
GROUP BY user_bill.id_user
ORDER BY total) as bill
WHERE us.id = bill.id_user and us.id_account=acc.id
limit $P{limit_top}]]>
	</queryString>
